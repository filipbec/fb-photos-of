{
  "manifest_version": 2,

  "name": "Facebook - photo of",
  "description": "Old version of 'photos of' feature on Facebook",
  "version": "1.1",
  "icons": {
    "128": "icons/icon.png"
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "page": "popup.html",
    "persistent": false
  }
}
